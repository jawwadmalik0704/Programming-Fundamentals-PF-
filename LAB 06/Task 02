#include <stdio.h>

int main() {
    int nnn, count = 0, sum = 0;

    printf("Enter a number: ");
    scanf("%d", &nnn);

    printf("Prime numbers up to %d are:\n", nnn);

    for (int i = 2; i <= nnn; i++) {
        int isPrime = 1;  // Assume i is prime

        for (int j = 2; j <= i - 1; j++) {
            if (i % j == 0) {
                isPrime = 0;  // Not prime
            }
        }

        if (isPrime) {
            printf("%d ", i);
            count++;
            sum += i;
        }
    }

    printf("\nTotal prime numbers up to %d: %d\n", nnn, count);
    printf("Sum of prime numbers up to %d: %d\n", nnn, sum);

    return 0;
}
